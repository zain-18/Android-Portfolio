        <activity android:name="com.locantoapps.soundboard.Page4" />
        <activity android:name="com.locantoapps.soundboard.Page3" />
        <activity android:name="com.locantoapps.soundboard.Page2" />
        <activity android:name="com.locantoapps.soundboard.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.locantoapps.soundboard.MainScreen" />
        <activity android:name="com.locantoapps.soundboard.MainActivity" />

